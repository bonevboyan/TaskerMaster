<div class="sidebar">
        <small class="text-muted pl-3">Chats</small>
        <ul>
            @foreach(var chat in @Model.Chats){
                <li>
                    <a href="chats/@(ViewBag.TeamId ?? "@me" )/@chat.Id">
                        <p> @chat.Name</p>
                    </a>
                </li>
            }
        </ul>
    </div>